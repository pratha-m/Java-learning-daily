<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="in.sp.beans.Student" id="stdId1"> <!--Crreate object of student class with id stdId-->
    	<property name="name" value="Pratham"/>
    	<property name="rollno" value="101"/>
    	<property name="email" value="pratham@gmail.com"/>
    </bean>
    
    <bean class="in.sp.beans.Student" id="stdId2"> <!--Crreate object of student class with id stdId-->
    	<property name="name" value="Rahul"/>
    	<property name="rollno" value="234"/>
    	<property name="email" value="rahul@gmail.com"/>
    </bean>

</beans>